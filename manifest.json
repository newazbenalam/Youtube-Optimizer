  {
    "name": "Youtube Optimizer",
    "version": "1.0",
    "description": "Removes unnesseary component and optimzed your browser!",
    "permissions": [
      "declarativeContent",
      "activeTab",
      "storage",
      "<all_urls>",
      "background"],
    "options_page": "options.html",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "author": "Newaz Ben Alam",
      "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
      "content_scripts":
        {
          "matches": [
          "*.youtube.com",
          "google.com",
          "edge://extensions/"]
        }
    },
	    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }